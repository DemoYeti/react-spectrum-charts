{
	"name": "@aaui/prism",
	"version": "0.1.7",
	"description": "Declarative library for composing Spectrum visualizations in React.",
	"type": "module",
	"main": "./dist/cjs/app.module.js",
	"module": "./dist/cjs/app.module.js",
	"types": "./dist/@types/src/index.d.ts",
	"files": [
		"dist/@types",
		"dist/cjs",
		"dist/app.module.js",
		"dist/app.module.js.LICENSE.txt",
		"dist/app.module.js.map",
		"dist/index.html"
	],
	"repository": "git@github.com:adobe/react-spectrum-charts.git",
	"author": "Marshall Peterson <mpeterson@adobe.com>",
	"license": "MIT",
	"scripts": {
		"aladdin:put": "cross-env NODE_ENV='production' && aladdin --command put",
		"aladdin:get": "cross-env NODE_ENV='production' && aladdin --command get",
		"build": "concurrently yarn:build:*",
		"build:mfe": "webpack --mode=production --env prod=true --env target=mfe",
		"build:cjs": "webpack --mode=production --env prod=true --env target=cjs",
		"build:storybook": "build-storybook -o ./dist/storybook --quiet",
		"copyrights": "babel-node --presets @babel/env ./scripts/addHeaders.js",
		"g": "plop component",
		"generate": "plop component",
		"lint": "eslint src --ext ts",
		"prepack": "yarn build",
		"pack-test": "rm -rf dist & npm pack",
		"start": "webpack serve --port=9026 --env target=mfe",
		"storybook": "start-storybook -p 6009",
		"test": "cross-env BABEL_ENV=test jest",
		"watch": "cross-env BABEL_ENV=test jest --watch",
		"skulk": "yarn watch --silent"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
